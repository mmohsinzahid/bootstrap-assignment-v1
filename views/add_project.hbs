{{> header}}
<div>
    <h3 class="h3_main">Add Project</h3>
    <div class="border rounded white-box">
    <form method="post">
        <div class="form-group">
            <label>Title</label>
            <input type="text" autocomplete="false" placeholder="Project Name" id="txt_projectname" class="form-control" />
        </div>
        <div class="form-group">
            <label>Description</label>
            <textarea id="ta_desc" class="form-control"></textarea>
        </div>
        <div class="form-group">
            <label>Team Lead</label>
            <select id="sl_tl" class="form-control">
                <option value="">Select Team</option>
                <option value="1">Amir</option>
                <option value="2">Waseem</option>
                <option value="3">Waqar</option>
                <option value="4">Saeed</option>
                <option value="5">Ijaz</option>
            </select>
        </div>
        <div class="form-group">
            <label>Members</label>
            <select id="txt_email" class="form-control" multiple>
                <option value='1'>Sarraz</option>
                <option value='2'>Shaheen</option>
                <option value='3'>Shan</option>
                <option value='4'>Raihan</option>
                <option value='5'>Qasim</option>
            </select>
        </div>
        <div class="form-group">
            <label>Documents</label>
            <input type="file" name="file_doc" id="file_doc">
            <span class="icon-plus fas fa-plus"></span>
        </div>
        <div class="form-group float-right">
            <input type="reset" value="Cancel">
            <button name="btn_submit" class="btn btn-primary" style="">Add Project</button>
        </div>
        <div class="clearfix"></div>
    </form>
    </div>
</div>
{{> footer}}